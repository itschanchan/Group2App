<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Group2App" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/brightPurple</item>
        <item name="colorPrimaryContainer">@color/darkPurple</item>
        <item name="colorSecondary">@color/amaranth</item>
    </style>

    <style name="Theme.Group2App" parent="Base.Theme.Group2App" />

    <style name="Theme.Group2App.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Group2App.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Group2App.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>